<section class="section results-section">
	<div class="container">
		<div class="section-header fade-in">
			<span class="section-label">Palmarès</span>
			<h1 class="section-title">Résultats</h1>
			<p class="section-description">Tous les résultats depuis 2000</p>
		</div>

		<!-- Statistiques globales -->
		<div class="results-stats fade-in" id="results-stats"></div>

		<!-- Filtres -->
		<div class="results-filters fade-in" id="results-filters">
			<div class="filter-group">
				<label for="filter-year">Année</label>
				<select id="filter-year" class="filter-select">
					<option value="">Toutes les années</option>
				</select>
			</div>
			<div class="filter-group">
				<label for="filter-circuit">Circuit</label>
				<select id="filter-circuit" class="filter-select">
					<option value="">Tous les circuits</option>
				</select>
			</div>
			<div class="filter-group">
				<label for="filter-competition">Compétition</label>
				<select id="filter-competition" class="filter-select">
					<option value="">Toutes les compétitions</option>
				</select>
			</div>
			<div class="filter-group">
				<label for="filter-categorie">Catégorie</label>
				<select id="filter-categorie" class="filter-select">
					<option value="">Toutes les catégories</option>
				</select>
			</div>
			<div class="filter-group">
				<label for="filter-podium">Podiums uniquement</label>
				<select id="filter-podium" class="filter-select">
					<option value="">Tous les résultats</option>
					<option value="true">Podiums seulement</option>
				</select>
			</div>
			<button type="button" class="filter-reset" id="filter-reset">Réinitialiser</button>
		</div>

		<div class="results-count fade-in" id="results-count"></div>

		<div id="resultats-container" class="fade-in">
			<p class="loading-text">Chargement des résultats...</p>
		</div>
	</div>
</section>

<section class="section section-alt">
	<div class="container">
		{% include "partners.njk" %}
	</div>
</section>
